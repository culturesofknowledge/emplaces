        em:relationTo ex:Opole_Duchy ;
        em:when 
          [ a em:Time_period ;
            rdfs:label   "1281-1521" ;
            em:timespan
              [ em:start  "1281" ;
                em:end    "1521" ;
              ]
          ] ;
        # em:source ????
        em:competence em:DEFINITIVE ;
      ] ,
      [ a em:Qualified_relation ;
        em:relationType em:AH_PART_OF_AH ;
        em:relationTo ex:Opole_Raciborz_Duchy ;
